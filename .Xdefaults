URxvt.depth: 32

URxvt.saveLines: 32767
URxvt.scrollBar: false

# tabs and links
#URxvt.perl-ext-common: default,tabbed,matcher
URxvt.perl-ext-common: default,matcher,clipboard,url-select,keyboard-select
URxvt.urlLauncher: /usr/bin/google-chrome
URxvt.matcher.button: 1

#URxvt*font: xft:inconsolata:pixelsize=12:antialias=true:hinting=true
URxvt.font: -*-terminus-medium-*-*-*-16-*-*-*-*-*-*-*
#Uxvt.boldfont: -*-terminus-bold-*-*-*-14-*-*-*-*-*-*-*
URxvt*font: xft:m+ 1mn medium:pixelsize=16:antialias=true:hinting=true
URxvt*font: xft:m+ 1mn regular:pixelsize=16:antialias=true:hinting=true
#URxvt*boldfont: xft:m+ 1mn bold:pixelsize=13:antialias=true:hinting=true

URxvt*foreground: #f2f2f2
#URxvt*background: #101010
URxvt*background: #000000

